{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/npc/obj_dir/*",
                "abstract-machine/am/include/arch/*",
                "abstract-machine/am/include/*",
                "${workspaceFolder}/nemu/include/**",
                "${workspaceFolder}/navy-apps/**",
                "nanos-lite/include/*",
                "${workspaceFolder}/navy-apps/libs/libminiSDL/include/*",
